# This file was generated with the createWorkflows.cmake script.

name: @configName@

on: [push, pull_request]

jobs:
  Windows:
    runs-on:  windows-latest
    strategy:
      fail-fast: false
    steps:
      - name: install
        run: pip3 install conan
      - uses: actions/checkout@v2
      - name: run
        run: |
          conan remote add bincrafters https://api.bintray.com/conan/bincrafters/public-conan 
          conan install Sources --profile "Sources/ConanProfiles/@conanProfile@" --install-folder build --build=missing --options qtVersion=@qtVersion@
          conan build Sources --build-folder build
